{
    "name": "perimeterit/scitalk",
    "description": "Video talks base project from Perimeter Institute",
    "type": "drupal-custom-profile",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/admin_toolbar": "^2.0",
        "drupal/backup_migrate": "^4.1",
        "drupal/console": "^1.0.2",
        "drupal/consumers": "^1.10",
        "drupal/copyright_block": "^1.0@RC",
        "drupal/core": "^8.8.0",
	"drupal/date_all_day": "1.x-dev",
        "drupal/devel": "^2.1",
        "drupal/embed": "^1.3",
        "drupal/entity_embed": "^1.1",
        "drupal/features": "^3.8",
        "drupal/fences": "^2.0",
        "drupal/field_group": "^3.0",
        "drupal/inline_entity_form": "^1.0",
        "drupal/jsonapi_extras": "^3.14",
        "drupal/jw_player": "^1.0@beta",
        "drupal/module_filter": "^3.1",
        "drupal/pathauto": "^1.6",
        "drupal/recaptcha": "^2.4",
        "drupal/restui": "^1.18",
        "drupal/search_api": "^1.15",
        "drupal/search_api_solr": "^3.8",
        "drupal/simple_oauth": "^4.3",
        "drupal/starbase": "1.x-dev",
        "drush/drush": "^9.3",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "drupal/core-dev": "^8.8.0",
        "symfony/debug": "^3.4"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "vendor-dir": "./vendor"
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-paths": {
            "core": ["type:drupal-core"],
            "libraries/{$name}": ["type:drupal-library"],
            "modules/contrib/{$name}": ["type:drupal-module"],
            "profiles/contrib/{$name}": ["type:drupal-profile"],
            "themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "patches": {
        }
    }
}
